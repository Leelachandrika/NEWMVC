@model IEnumerable<CargoManagement.gatePass>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.city.state)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.truck_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.issued_date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.city.state)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.truck_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.issued_date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.number }) |
            @Html.ActionLink("Details", "Details", new { id=item.number }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.number })
        </td>
    </tr>
}

</table>
